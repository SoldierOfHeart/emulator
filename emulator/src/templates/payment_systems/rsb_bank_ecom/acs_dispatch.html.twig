<!DOCTYPE html SYSTEM "about:legacy-compat">
<html class="no-js" lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8"/>
    <title>3D Secure Processing</title>
    <link href="https://testsecurepay.rsb.ru/mdpaympi/static/mpi.css" rel="stylesheet" type="text/css"/>
</head>
<body>
<div id="main">
    <div id="content">
        <div id="order">
            <h2>3D Secure Processing</h2>
            <img src="https://testsecurepay.rsb.ru/mdpaympi/static/preloader.gif" alt="Please wait.."/>
            <img src="https://testsecurepay.rsb.ru/mdpaympi/static/verifiedbyvisa.png" alt="Verified by VISA"/>
            <div id="formdiv">
                <script type="text/javascript">
                    function hideAndSubmitTimed(formid)
                    {
                        var timer=setTimeout("hideAndSubmit('"+formid+"');",100);
                    }

                    function hideAndSubmit(formid)
                    {
                        var formx=document.getElementById(formid);
                        if (formx!=null)
                        {
                            formx.style.visibility="hidden";
                            formx.submit();
                        }
                    }
                </script>
                <div>
                    <form id="webform0" name="red2Mer" method="POST" action="https://paybox.kz/payment/dispatch/RSBECOMRUB?customer=<test>" accept_charset="UTF-8">
                        <input type="hidden" name="_charset_" value="UTF-8"/>
                        <input type="hidden" name="version" value="2.0"/>
                        <input type="hidden" name="merchantID" value="{{ merchantID }}"/>
                        <input type="hidden" name="xid" value="{{ xid }}="/>
                        <input type="hidden" name="mdStatus" value="{{ mdStatus }}"/>
                        <input type="hidden" name="mdErrorMsg" value="{{ mdErrorMsg }}"/>
                        <input type="hidden" name="txstatus" value="{{ txstatus }}"/>
                        <input type="hidden" name="iReqCode" value=""/>
                        <input type="hidden" name="iReqDetail" value=""/>
                        <input type="hidden" name="vendorCode" value=""/>
                        <input type="hidden" name="eci" value="{{ eci }}"/>
                        <input type="hidden" name="cavv" value="{{ cavv }}="/>
                        <input type="hidden" name="cavvAlgorithm" value="{{ cavvAlgorithm }}"/>
                        <input type="hidden" name="MD" value=""/>
                        <input type="hidden" name="PAResVerified" value="{{ PAResVerified }}"/>
                        <input type="hidden" name="PAResSyntaxOK" value="{{ PAResSyntaxOK }}"/>
                        <input type="text" name="digest" value="{{ digest }}=" readonly="true" style="display:none;"/>
                        <input type="hidden" name="sID" value="{{ sID }}"/>
                        <input type="submit" name="submitBtn" value="Please click here to continue"/>
                    </form>
                </div>
            </div>
            <script type="text/javascript">
                hideAndSubmitTimed('webform0');
            </script>
            <noscript>
                <div align="center">
                    <b>Javascript is turned off or not supported!</b>
                    <br/>
                </div>
            </noscript>
        </div>
        <div id="content-footer">
            <br/>
            <img height="20" src="https://testsecurepay.rsb.ru/mdpaympi/static/powered-by-modirum.svg" alt="Powered by Modirum"/>
        </div>
    </div>
</div>
</body>
</html>